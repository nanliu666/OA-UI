{
  "name": "mage-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "yarn || npm i",
    "build": "npm run build:file && vue-cli-service build --target lib --name mage --dest dist ./src/index.js",
    "build:file": "node build/bin/build-entry.js",
    "serve": "npm run bootstrap && npm run build:file && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.demo.js",
    "lint": "eslint --fix --ext .js examples/**/* packages/**/* build/**/* src/**/*"
  },
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "peerDependencies": {
    "element-ui": "^2.13.2",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "element-ui": "^2.13.2",
    "eslint": "^6.7.2",
    "eslint-plugin-html": "^6.0.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "highlight.js": "^10.1.2",
    "husky": "^4.2.5",
    "json-templater": "^1.2.0",
    "lint-staged": "^10.2.13",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "transliteration": "^2.1.11",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^4.1.0",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.3",
    "vue-router": "^3.4.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix --quiet"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
